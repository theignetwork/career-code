# Career Code Assessment Environment Variables

# Development/Production Environment
NODE_ENV=production

# Analytics and Tracking
VITE_ANALYTICS_ID=
VITE_GOOGLE_ANALYTICS_ID=

# Email Service Configuration
VITE_EMAIL_SERVICE_URL=
VITE_EMAIL_API_KEY=

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_EMAIL_CAPTURE=true
VITE_ENABLE_SOCIAL_SHARING=true

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true

# iframe Embedding Configuration
VITE_IFRAME_ALLOWED_ORIGINS=*
VITE_IFRAME_RESIZE_ENABLED=true

# Build Configuration
VITE_BUILD_TARGET=es2015
VITE_BASE_PATH=/